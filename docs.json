{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Rover Docs",
  "colors": {
    "primary": "#212121",
    "light": "#212121",
    "dark": "#e4e4e4"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/overview/rover"
    }
  ],
  "navigation": {
    "groups": [
      {
        "group": "Overview",
        "pages": [
          "overview/rover",
          "overview/use-cases"
        ]
      },
      {
        "group": "Get Started",
        "pages": [
          "get-started/welcome",
          "get-started/concepts",
          "get-started/examples"
        ]
      },
      {
        "group": "Core",
        "pages": [
          "core/command-menu",
          "core/tables",
          "core/detail-pages",
          "core/views",
          "core/labels-bookmarks",
          "core/sharing",
          "core/personalization"

        ]
      },
      {
        "group": "Contact",
        "pages": [
          "contact/book-a-demo",
          "contact/email"
        ]
      }
    ],
    "global": {
    
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
  
    "primary": {
      "type": "button",
      "label": "Join Beta",
      "href": "https://rover.inc"
    }
  }
  
}


